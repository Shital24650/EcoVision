{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "pages/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/", "dest": "/pages/index.html" },
    { "src": "/coach", "dest": "/pages/coach.html" },
    { "src": "/carbon", "dest": "/pages/carbon.html" },
    { "src": "/trackert", "dest": "/pages/trackert.html" }
  ]
}
